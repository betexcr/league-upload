{
  "name": "league-upload",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:all": "node scripts/dev-all.js",
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "typecheck": "tsc -b --pretty false",
    "panda:generate": "panda codegen && panda cssgen",
    "panda:watch": "panda codegen --watch"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.9.1",
    "@pandacss/dev": "^0.41.0",
    "@pandacss/preset-panda": "^0.41.0",
    "@types/express": "^5.0.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "jest": "^29.7.0",
    "prettier-eslint": "^16.4.2",
    "typescript": "^5.4.5",
    "vitest": "^1.3.1"
  },
  "msw": {
    "workerDirectory": [
      "apps\\web\\public"
    ]
  }
}
