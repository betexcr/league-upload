{
  "name": "@league/api",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:worker": "node dist/scan.worker.js",
    "build": "nest build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config jest.unit.config.js --runInBand",
    "test:watch": "jest --config jest.unit.config.js --watch",
    "test:cov": "jest --config jest.unit.config.js --coverage",
    "test:e2e": "jest --config jest.e2e.config.js --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.535.0",
    "@aws-sdk/client-sqs": "^3.535.0",
    "@aws-sdk/s3-request-presigner": "^3.535.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.10",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.19",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "prisma": "^5.11.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  }
}
