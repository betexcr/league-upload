{
  "name": "@league/web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "predev": "npm --prefix ../.. run panda:generate",
    "dev": "vite",
    "build": "npm --prefix ../.. run panda:generate && tsc -b ../../packages/types ../../packages/upload-core ../../packages/upload-ui ../../packages/doc-viewer ../../packages/app-client && vite build",
    "preview": "vite preview",
    "test": "vitest run --exclude e2e/**",
    "test:watch": "vitest --watch --exclude e2e/**",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@fontsource/ibm-plex-mono": "^5.1.0",
    "@fontsource/ibm-plex-sans": "^5.1.0",
    "@league/app-client": "0.1.0",
    "@league/upload-ui": "0.1.0",
    "@tanstack/react-query": "^5.51.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.48.2",
    "jsdom": "^24.0.0",
    "msw": "^2.3.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.3.1"
  }
}
